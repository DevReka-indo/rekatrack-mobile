{
  "expo": {
    "name": "rekatrack-mobile",
    "slug": "rekatrack-mobile",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.mobile.png",
    "scheme": "rekatrack-mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.reka.rekatrackmobile",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Aplikasi membutuhkan akses kamera untuk mengambil foto bukti pengiriman.",
        "NSPhotoLibraryUsageDescription": "Aplikasi membutuhkan akses galeri untuk memilih foto bukti pengiriman.",
        "NSPhotoLibraryAddUsageDescription": "Aplikasi dapat menyimpan hasil foto bukti pengiriman bila diperlukan."
      }
    },
    "android": {
      "package": "com.reka.rekatrackmobile",
      "icon": "./assets/images/logo.mobile.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplikasi membutuhkan akses galeri untuk memilih foto bukti pengiriman.",
          "cameraPermission": "Aplikasi membutuhkan akses kamera untuk mengambil foto bukti pengiriman."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "200d2d34-f72d-48a7-a723-d117f19be0cc"
      }
    },
    "owner": "fajarbudhi17",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/200d2d34-f72d-48a7-a723-d117f19be0cc"
    }
  }
}
